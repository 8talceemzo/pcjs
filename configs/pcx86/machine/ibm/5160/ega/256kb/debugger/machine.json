{
  "@title": "IBM PC XT (Model 5160) with Enhanced Color Display and Debugger",
  "defaults": {
    "ram": "640K",
    "fpu": "None",
    "fdc": "2 diskette drives",
    "parallel": "LPT1",
    "serial": "COM1,COM2",
    "mouse": "COM2"
  },
  "components": [
    {
      "id": "pcEGA",
      "class": "computer"
    },
    {
      "id": "debugger",
      "class": "debugger"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/rom-basic-110.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/rom-bios-rev1.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/rom-ega.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/rom-hdc.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/video-ega-5154-128k.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/fdc.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/hdc.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5160/ram.json",
      "test": "false"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5150/cpu.json"
    },
    {
      "config": "/configs/pcx86/machine/ibm/5150/keyboard.json"

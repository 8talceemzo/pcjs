      "id": "lpt1",
      "class": "parallel",
      "adapter": 2,
      "binding": "print"
    },
    {
      "id": "com1",
      "class": "serial",
      "adapter": 1,
      "binding": "print"
    },
    {
      "id": "com2",
      "class": "serial",
      "adapter": 2
    },
    {
      "id": "mouse",
      "class": "mouse",
      "serial": "com2"
    }
  ]
}

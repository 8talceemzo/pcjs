{
  "@title": "IBM PC XT (Model 5160) with Monochrome Display and Debugger",
  "defaults": {
    "ram": "256K",
    "fpu": "None",
    "fdc": "2 diskette drives",
    "parallel": "LPT1",
    "serial": "COM1,COM2",
    "mouse": "COM2"
  },
  "components": [
    {
      "id": "pcMDA",
      "class": "computer"
    },
    {
      "id": "debugger",
      "class": "debugger"
    },
    {
      "config": "/configs/pcx86/ibm/5160/rom-basic-110.json"
    },
    {
      "config": "/configs/pcx86/ibm/5160/rom-bios-rev1.json"
    },
    {
      "config": "/configs/pcx86/ibm/5160/rom-hdc.json"
    },
    {
      "config": "/configs/pcx86/ibm/5150/video-mda.json"
    },
    {
      "config": "/configs/pcx86/ibm/5160/fdc.json"
    },
    {
      "config": "/configs/pcx86/ibm/5160/hdc.json"
    },
    {
      "config": "/configs/pcx86/ibm/5160/ram.json",
      "test": "false"
    },
    {
      "config": "/configs/pcx86/ibm/5150/cpu.json"
    },
    {
      "config": "/configs/pcx86/ibm/5150/keyboard.json"
    },
    {
      "config": "/configs/pcx86/ibm/5150/panel.json"
    },
    {
      "config": "/configs/pcx86/ibm/5160/chipset.json",
      "sw1": "01000001"
    },
    {
      "id": "lpt1",
      "class": "parallel",
      "adapter": 2,
      "binding": "print"
    },
    {
      "id": "com1",
      "class": "serial",
      "adapter": 1,
      "binding": "print"
    },
    {
      "id": "com2",
      "class": "serial",
      "adapter": 2
    },
    {
      "id": "mouse",
      "class": "mouse",
      "serial": "com2"
    }
  ]
}

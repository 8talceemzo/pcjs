    "version": "0.2.0",
    "configurations": [
      {
        "name": "Node: gulp.js",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopOnEntry": false,
        "console": "internalConsole"
      },
      {
        "name": "Node: diskettes.js",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/tools/misc/diskettes.js",
